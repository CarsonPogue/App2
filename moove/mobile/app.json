{
  "expo": {
    "name": "MOOVE",
    "slug": "moove",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "moove",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D1A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moove.app",
      "config": {
        "googleMapsApiKey": "AIzaSyCX5tl_UFwXrdevtoD6Az6KBSa4aoaemVI"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "MOOVE uses your location to show events near you.",
        "NSLocationAlwaysUsageDescription": "MOOVE uses your location to notify you about nearby events."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#0D0D1A"
      },
      "package": "com.moove.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCX5tl_UFwXrdevtoD6Az6KBSa4aoaemVI"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "MOOVE uses your location to show events near you."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
